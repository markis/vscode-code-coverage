{
  "name": "code-coverage",
  "displayName": "Code Coverage",
  "description": "Tool to highlight lines not covered by unit tests.",
  "version": "1.1.0",
  "publisher": "markis",
  "icon": "images/icon.png",
  "license": "MIT",
  "engines": {
    "vscode": "^1.30.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markis/vscode-code-coverage.git"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "code coverage",
    "lcov",
    "unit test"
  ],
  "activationEvents": [
    "workspaceContains:**/lcov*.info",
    "onLanguage:clojure",
    "onLanguage:coffeescript",
    "onLanguage:c",
    "onLanguage:cpp",
    "onLanguage:csharp",
    "onLanguage:fsharp",
    "onLanguage:go",
    "onLanguage:haxe",
    "onLanguage:java",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:objective-c",
    "onLanguage:objective-cpp",
    "onLanguage:perl",
    "onLanguage:perl6",
    "onLanguage:php",
    "onLanguage:python",
    "onLanguage:ruby",
    "onLanguage:rust",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vb"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "configuration": {
      "title": "Code coverage",
      "properties": {
        "markiscodecoverage.searchCriteria": {
          "type": "string",
          "default": "coverage/lcov*.info",
          "description": "Search location for lcov files"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "compile:watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/run-test.js"
  },
  "devDependencies": {
    "@types/glob": "7.1.1",
    "@types/mocha": "7.0.1",
    "@types/node": "13.7.0",
    "@types/vscode": "1.41.0",
    "glob": "7.1.6",
    "mocha": "7.1.0",
    "typescript": "3.7.5",
    "vscode-test": "1.3.0"
  },
  "dependencies": {}
}
