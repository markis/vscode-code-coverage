{
  "compilerOptions":{
    "module": "commonjs",
    "target": "es6",
    "outDir": "out",
    "lib":[
      "es6"
    ],
    "sourceMap": true,
    "rootDir": ".",

    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude":[
    "node_modules",
    ".vscode-test"
  ]
}